{
  "name": "taffy",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.84.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kapydev/taffy"
  },
  "publisher": "firejet",
  "scripts": {
    "serve": "nx run-many --target=serve --all",
    "init:services": "npx supabase start",
    "db:types": "npx supabase gen types typescript --local > shared/types/src/supabase-types.ts",
    "db:reset": "supabase db reset && npm run db:types",
    "build": "nx run apps-ui:build && nx run vsc-ext:build --skip-nx-cache",
    "package": "npm run build && npx vsce package --allow-missing-repository",
    "publish": "npm run build && npx vsce publish"
  },
  "main": "./dist/apps/vsc-ext/main.cjs",
  "activationEvents": [
    "onStartupFinished"
  ],
  "files": [
    "LICENSE.md",
    "dist/apps/vsc-ext/**/*"
  ],
  "contributes": {
    "keybindings": [
      {
        "command": "taffy.init",
        "key": "ctrl+shift+m",
        "when": "editorTextFocus"
      }
    ]
  },
  "private": true,
  "dependencies": {
